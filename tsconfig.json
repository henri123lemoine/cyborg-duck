{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "ES6",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "lib": [
      "DOM",
      "ES5",
      "ES6",
      "ES7"
    ],
    "typeRoots": ["./node_modules/@types", "./node_modules/obsidian/types"],
    "paths": {
      "obsidian": ["./node_modules/obsidian/obsidian.d.ts"]
    }
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "tmp files/*"
  ]
}